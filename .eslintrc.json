{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-unused-vars": "off",
        "no-param-reassign": [
            "error",
            {
              "props": true,
              "ignorePropertyModificationsFor": [
                "state"
              ]
            }
        ]
    }
}
